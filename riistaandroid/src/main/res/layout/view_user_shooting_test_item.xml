<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="@dimen/item_spacing_normal">

    <TextView
        android:id="@+id/user_shooting_test_type"
        style="@style/PrimaryTextBody.Bold"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="Test type, bow or regular" />

    <TextView
        android:id="@+id/user_shooting_test_rhy"
        style="@style/PrimaryTextBody"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/user_shooting_test_type"
        tools:text="RHY localised name" />

    <TextView
        android:id="@+id/user_shooting_test_valid_dates"
        style="@style/PrimaryTextBody"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@id/user_shooting_test_rhy"
        tools:text="00.00.1900 - 00.00.1901" />

</androidx.constraintlayout.widget.ConstraintLayout>
